@model IEnumerable<RightCareSite.Models.DataBase.MainStore>

@{
    ViewBag.Title = "ProdMotion";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var prod=Model.Select(x => x.Product_Tble.Name).FirstOrDefault();
    var prodCod = Model.Select(x => x.Product_TbleId).FirstOrDefault();

}

<h2>حركة صنف في المخزن</h2>
<div class="col-md-6">
    <h3>@prodCod</h3> 
</div>
<div class="col-md-6">
    <h3>@prod</h3>
</div>
  

<table class="table">
    <tr>

        <th>
التاريخ
        </th>
        <th>
            وارد مندوب
        </th>
        <th>
            منصرف لمندوب
        </th>
        <th>
            مردود مشتريات
        </th>
        <th>
            مشتريات
        </th>
       
        <th>
            منصرف
        </th>
        <th>
            وارد
        </th>
       
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MndStkOutId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MndStkInId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReBUy_tblId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Buy_tblId)
        </td>
      
        <td>
            @Html.DisplayFor(modelItem => item.QtyOut)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.QtyIn)
        </td>
      
    </tr>
}

</table>
