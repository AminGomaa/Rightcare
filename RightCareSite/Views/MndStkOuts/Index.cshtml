@model IEnumerable<RightCareSite.Models.DataBase.MndStkOut>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>مرتجعات المندوبين</h2>

<p>
    @Html.ActionLink("مرتجع مندوب", "Create")
</p>
<table class="table">
    <tr>
        <th>
كود المندوب
        </th>
        <th>
المندوب
        </th>
        <th>
التاريخ
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MndId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MndName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrderDate)
        </td>
        <td>
            @Html.ActionLink("تفاصيل", "Details", new { id=item.Id }) 
        </td>
    </tr>
}

</table>
