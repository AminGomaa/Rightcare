@model IEnumerable<RightCareSite.Models.DataBase.MndStkIn>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2> اذونات صرف بضاعة للمندوبين</h2>

<p>
    @Html.ActionLink("اذن صرف جديد", "Create")
</p>
<table class="table">
    <tr>
        <th>
كود المندوب
        </th>
        <th>
المندوب
        </th>
        <th>
التاريخ
        </th>
        <th>
            رقم اذن الصرف
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MndId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MndName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrderDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id = item.Id }) 
        </td>
    </tr>
}

</table>
